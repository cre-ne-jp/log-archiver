<% title('キーワード') %>
<div class="container main-container">
  <div class="row">
    <div class="col-xs-12">
      <div class="main-panel">
        <div class="panel-body">
          <%= render('shared/flash') %>

          <div class="page-header">
            <h1>キーワード</h1>
          </div>

          <ul>
            <% @keywords.each do |keyword| %>
              <li><%= link_to(h(keyword.display_title), keyword) %></li>
            <% end %>
          </ul>
        </div>
      </div>
    </div>
  </div>
</div>
