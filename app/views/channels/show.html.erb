<h1>チャンネル登録情報</h1>
<table>
  <tr>
    <th>#</th>
    <td><%= @channel.id %></td>
  </tr>
  <tr>
    <th>チャンネル名</th>
    <td><%= @channel.name_with_prefix %></td>
  </tr>
  <tr>
    <th>識別子</th>
    <td><%= @channel.identifier %></td>
  </tr>
  <tr>
    <th>ログ取得状況</th>
    <td><%= @channel.logging_enabled? ? '取得中' : '取得停止中' %></td>
  </tr>
</table>

<h1>ログ</h1>
<pre><%= @messages.map(&:to_tiarra_format).join("\n") %></pre>
