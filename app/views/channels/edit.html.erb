<% title('チャンネル情報の編集') %>
<%= render('shared/navbar') %>
<div class="container main-container">
  <%= render('shared/flash') %>

  <div class="row">
    <%= render('shared/admin/nav_channels') %>

    <div class="col-xs-12 col-sm-9">
      <ol class="breadcrumb">
        <li><%= link_to('管理', admin_path) %></li>
        <li><%= link_to('チャンネル', admin_channels_path) %></li>
        <li><%= link_to(@channel.name_with_prefix, @channel) %></li>
        <li class="active">編集</li>
      </ol>

      <div class="page-header">
        <h1>チャンネル情報の編集</h1>
      </div>

      <%= render('shared/error_messages', target: @channel) %>

      <%= render('edit_form') %>
    </div>
  </div>
</div>
